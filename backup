<script>
    function readMore() {
        var x = document.getElementById("readmore")
        if (x.style.display == "block") {
            document.getElementById("readMorebtn").innerHTML = "ReadMore"
            x.style.display = 'none';

        }
        else {
            document.getElementById("readMorebtn").innerHTML = "ReadLess"
            x.style.display = 'block';
        }

    }
</script>
